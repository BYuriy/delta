            </tab>
            <tab id="balanceDataTab" caption="msg://balanceDataTab.caption">
                <iframe screen="delta$MainReportBalanceDataFrame"/>
            </tab>
            <!--<tab id="priceDataTab" caption="msg://priceDataTab.caption">-->
